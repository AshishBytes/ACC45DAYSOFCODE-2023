// https://www.codechef.com/problems/SURPLUS

#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int A1, A2, B1, B2;
	    cin>>A1>>A2>>B1>>B2;
	    int NA = A1-A2;
	    int NB = B1-B2;
	    int NE = NA + NB;
	    if(NE < 0) cout<<"YES"<<endl;
	    else cout<<"NO"<<endl;
	}
	return 0;
}
